{
  "name": "responsive-props",
  "version": "0.0.0-development",
  "description": "Responsive props for Styled Components",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "npm-run-all --parallel build:*",
    "build:es6": "babel src --out-dir lib --ignore spec.js --source-maps",
    "build-umd": "webpack",
    "build-storybook": "build-storybook",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "cz-commit": "git-cz",
    "report-coverage": "codecov",
    "setup": "npm-run-all setup:*",
    "setup:semantic-release": "semantic-release-cli setup",
    "setup:commitizen": "commitizen init cz-conventional-changelog --save-dev --save-exact",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": "https://github.com/johnnyBira/responsive-props.git",
  "keywords": [
    "react",
    "styled-components",
    "utils",
    "HOC",
    "plugin",
    "styles",
    "css",
    "css-in-js",
    "inline-styles",
    "breakpoint",
    "breakpoints",
    "media-query",
    "media-queries",
    "responsive",
    "props"
  ],
  "author": "John Persson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnnyBira/responsive-props/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/test-bundler.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 88,
        "branches": 80,
        "functions": 76,
        "lines": 90
      }
    },
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "homepage": "https://github.com/jonnyBira/responsive-props#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/cli": "^8.0.0",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-backgrounds": "^5.3.0",
    "@storybook/addon-info": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addon-options": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "babel-loader": "^8.0.5",
    "codecov": "^3.2.0",
    "commitizen": "^3.0.7",
    "commitlint": "^8.0.0",
    "core-js": "3",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^2.4.1",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "jest-enzyme": "^7.0.2",
    "jest-styled-components": "next",
    "jsdom": "^15.1.1",
    "npm-run-all": "^4.1.1",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^5.0.0",
    "styled-components": "^4.2.0",
    "travis-deploy-once": "^5.0.11",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">= 16.3.0 < 17.0.0-0",
    "styled-components": ">= 4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "scope": {
        "required": true,
        "validate": true,
        "multiple": true
      }
    }
  },
  "dependencies": {
    "@humblebee/styled-components-breakpoint": "^2.1.3",
    "isobject": "^4.0.0"
  }
}
